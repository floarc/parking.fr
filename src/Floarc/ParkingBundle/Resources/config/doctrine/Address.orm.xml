<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Floarc\ParkingBundle\Entity\Address" table="address">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="address1" type="string" column="address1" length="225" nullable="true"/>
    <field name="address2" type="string" column="address2" length="255" nullable="true"/>
    <field name="address3" type="string" column="address3" length="255" nullable="true"/>
    <field name="codePostal" type="string" column="code_postal" length="5" nullable="true"/>
    <field name="ville" type="string" column="ville" length="255" nullable="true"/>
    <field name="departement" type="string" column="departement" length="255" nullable="true"/>
    <field name="region" type="string" column="region" length="255" nullable="true"/>
    <field name="pays" type="string" column="pays" length="255" nullable="true"/>
    <field name="lat" type="float" column="lat" precision="10" scale="0" nullable="true"/>
    <field name="lng" type="float" column="lng" precision="10" scale="0" nullable="true"/>
    <field name="alt" type="float" column="alt" precision="10" scale="0" nullable="true"/>
    <field name="createdAt" type="datetime" column="created_at" nullable="true"/>
    <field name="modifiedAt" type="datetime" column="modified_at" nullable="true"/>
  </entity>
</doctrine-mapping>
